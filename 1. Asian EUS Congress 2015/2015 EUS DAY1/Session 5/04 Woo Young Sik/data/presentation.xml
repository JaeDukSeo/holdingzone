		<slide index="9" title="Classification of results" titleNormalized="classification of results" src="slide10.swf" size="2759" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset9" presenterIndex="0">
