		<slide index="4" title="Pancreatic Necrosis" titleNormalized="pancreatic necrosis" src="slide5.swf" size="1821" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset4" presenterIndex="0">
