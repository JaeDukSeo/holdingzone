		<slide index="15" title="Appropriateness Voting" titleNormalized="appropriateness voting" src="slide16.swf" size="4562" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset15" presenterIndex="0">
			<sounds/>
