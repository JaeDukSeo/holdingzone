		<slide index="1" title="Precut quotes" titleNormalized="precut quotes" src="slide2.swf" size="2255" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset1" presenterIndex="0">
			<sounds/>
