		<slide index="1" title="Treatment for gallstone disease" titleNormalized="treatment for gallstone disease" src="slide2.swf" size="87847" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset1" presenterIndex="0">
