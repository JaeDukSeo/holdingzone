		<slide index="7" title="Definitions" titleNormalized="definitions" src="slide8.swf" size="1759" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset7" presenterIndex="0">
