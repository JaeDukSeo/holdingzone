		<slide index="1" title="Background" titleNormalized="background" src="slide2.swf" size="3012" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset1" presenterIndex="0">
