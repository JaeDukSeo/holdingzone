		<slide index="1" title="RCT EUS-FNA vs. EUS-FNB Introduction" titleNormalized="rct eus-fna vs. eus-fnb introduction" src="slide2.swf" size="27844" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset1" presenterIndex="0">
			<sounds/>
			<videos/>
			<transitionEffect name="" duration="0.0000000" soundId="" looped="0"/>
