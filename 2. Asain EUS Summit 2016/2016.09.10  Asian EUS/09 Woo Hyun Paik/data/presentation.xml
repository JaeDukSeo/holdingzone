		<slide index="2" title="Visceral artery dissection" titleNormalized="visceral artery dissection" src="slide3.swf" size="65124" advanceOnTime="1" framesCount="1" hasEmbeddedFlashClip="0" hidden="0" level="0" advanceOnClick="1" thumbnailAssetId="imgAsset2" presenterIndex="0">
